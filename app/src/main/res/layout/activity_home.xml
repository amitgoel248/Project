<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.project.amit.project.home">

    <include
        android:id="@+id/app_bar"
        layout="@layout/app_bar"></include>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/app_bar">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <android.support.v7.widget.CardView
                xmlns:card_view="http://schemas.android.com/apk/res-auto"
                android:id="@+id/card_view1"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                card_view:cardCornerRadius="4dp"
                />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:text="Getting used to Input//Output"/>

            <android.support.v7.widget.CardView
                xmlns:card_view="http://schemas.android.com/apk/res-auto"
                android:id="@+id/card_view2"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                card_view:cardCornerRadius="4dp"
                android:layout_below="@+id/card_view1"
                />

            <TextView
                android:layout_below="@id/card_view1"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:text="Brain Teasers"/>

            <android.support.v7.widget.CardView
                xmlns:card_view="http://schemas.android.com/apk/res-auto"
                android:id="@+id/card_view3"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                card_view:cardCornerRadius="4dp"
                android:layout_below="@+id/card_view2"
                />

            <TextView
                android:layout_below="@id/card_view2"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:text="STL(Standard Template Library)"/>

            <android.support.v7.widget.CardView
                xmlns:card_view="http://schemas.android.com/apk/res-auto"
                android:id="@+id/card_view4"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                card_view:cardCornerRadius="4dp"
                android:layout_below="@+id/card_view3"
                />
            <TextView
                android:layout_below="@id/card_view3"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:text="Optimisation"/>

            <android.support.v7.widget.CardView
                xmlns:card_view="http://schemas.android.com/apk/res-auto"
                android:id="@+id/card_view5"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                card_view:cardCornerRadius="4dp"
                android:layout_below="@+id/card_view4"
                />
            <TextView
                android:layout_below="@id/card_view4"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:text="Modulus and Overflow"/>

            <android.support.v7.widget.CardView
                xmlns:card_view="http://schemas.android.com/apk/res-auto"
                android:id="@+id/card_view6"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                card_view:cardCornerRadius="4dp"
                android:layout_below="@+id/card_view5"
                />
            <TextView
                android:layout_below="@id/card_view5"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:text="Number Theory"/>

            <android.support.v7.widget.CardView
                xmlns:card_view="http://schemas.android.com/apk/res-auto"
                android:id="@+id/card_view7"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                card_view:cardCornerRadius="4dp"
                android:layout_below="@+id/card_view6"
                />

            <TextView
                android:layout_below="@id/card_view6"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:text="Dynaminc Problem"/>
            <android.support.v7.widget.CardView
                xmlns:card_view="http://schemas.android.com/apk/res-auto"
                android:id="@+id/card_view8"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                card_view:cardCornerRadius="4dp"
                android:layout_below="@+id/card_view7"
                />
            <TextView
                android:layout_below="@id/card_view7"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:text="Greedy Approach"/>

            <android.support.v7.widget.CardView
                xmlns:card_view="http://schemas.android.com/apk/res-auto"
                android:id="@+id/card_view9"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                card_view:cardCornerRadius="4dp"
                android:layout_below="@+id/card_view8"
                />
            <TextView
                android:layout_below="@id/card_view8"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:text="Game Theory"/>

            <android.support.v7.widget.CardView
                xmlns:card_view="http://schemas.android.com/apk/res-auto"
                android:id="@+id/card_view10"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                card_view:cardCornerRadius="4dp"
                android:layout_below="@+id/card_view9"
                />
            <TextView
                android:layout_below="@id/card_view9"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:text="Graph Algorithms"/>

            <android.support.v7.widget.CardView
                xmlns:card_view="http://schemas.android.com/apk/res-auto"
                android:id="@+id/card_view11"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                card_view:cardCornerRadius="4dp"
                android:layout_below="@+id/card_view10"
                />
            <TextView
                android:layout_below="@id/card_view10"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:text="New Techniques"
                />

            <android.support.v7.widget.CardView
                xmlns:card_view="http://schemas.android.com/apk/res-auto"
                android:id="@+id/card_view12"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                card_view:cardCornerRadius="4dp"
                android:layout_below="@+id/card_view11"
                />
            <TextView
                android:layout_below="@id/card_view11"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:text="New Data Structures"
                />

        </RelativeLayout>
     </ScrollView>
        </RelativeLayout>
